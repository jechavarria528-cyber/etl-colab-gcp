      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from google.cloud import storage\n",
        "import io\n",
        "\n",
        "def main():\n",
        "    # Nombre del bucket y archivo\n",
        "    bucket_name = \"mi-bucket-prueba-jorge\"\n",
        "    input_file = \"datos.csv\"\n",
        "    output_file = \"resultado.csv\"\n",
        "\n",
        "    # Cliente de Cloud Storage\n",
        "    client = storage.Client()\n",
        "    bucket = client.bucket(bucket_name)\n",
        "\n",
        "    # Descargar archivo desde Cloud Storage\n",
        "    blob = bucket.blob(input_file)\n",
        "    data = blob.download_as_bytes()\n",
        "\n",
        "    # Leer CSV con pandas\n",
        "    df = pd.read_csv(io.BytesIO(data))\n",
        "\n",
        "    # Transformaci√≥n: calcular promedio de la edad\n",
        "    promedio = df[\"edad\"].mean()\n",
        "    print(f\"Promedio de edad: {promedio}\")\n",
        "\n",
        "    # Crear DataFrame con el resultado\n",
        "    df_resultado = pd.DataFrame({\"promedio_edad\": [promedio]})\n",
        "\n",
        "    # Guardar en memoria como CSV\n",
        "    output = io.StringIO()\n",
        "    df_resultado.to_csv(output, index=False)\n",
        "    output_data = output.getvalue().encode(\"utf-8\")\n",
        "\n",
        "    # Subir archivo resultado al bucket\n",
        "    blob_result = bucket.blob(output_file)\n",
        "    blob_result.upload_from_string(output_data, content_type=\"text/csv\")\n",
        "\n",
        "    print(f\"Resultado guardado en {output_file} en el bucket {bucket_name}\")\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
      ],
